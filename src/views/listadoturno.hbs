<div class="container">
    <table class="table table-dark">
        <thead>
           
            <th>Nombre</th>
            <th>Telefono</th>
            <th>Fecha</th>
            <th>Hora</th>
        </thead>
        <tbody>
            {{#each results}}
            <tr>
                {{!-- Para traer datos de la BS --}}
                <td>{{nombre}}</td>
                <td>{{telefono}}</td>
                <td>{{fecha}}</td>
                <td>{{hora}}</td>
            </tr>
            {{/each}}

        </tbody>
    </table>
</div> 